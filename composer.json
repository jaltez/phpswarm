{
    "name": "jaltez/phpswarm",
    "description": "A modern AI Agentic framework for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "ai",
        "agents",
        "llm",
        "openai",
        "framework"
    ],
    "authors": [
        {
            "name": "Javier Altez",
            "homepage": "https://github.com/jaltez"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "psr/log": "^3.0",
        "psr/container": "^2.0",
        "psr/http-client": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "symfony/http-client": "^7.0",
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.15",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PhpSwarm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSwarm\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "static": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable"
}